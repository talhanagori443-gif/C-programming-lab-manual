#include <stdio.h>
int main() {
    int start, end, i, sum = 0;

    printf("Enter the first number: ");
    scanf("%d", &start);

    printf("Enter the second number: ");
    scanf("%d", &end);

    // Ensure start < end
    if (start > end) {
        int temp = start;
        start = end;
        end = temp;
    }

    for (i = start; i <= end; i++) {
        if (i % 2 != 0) {
            sum = sum + i;
        }
    }

    printf("Sum of odd numbers between %d and %d = %d\n", start, end, sum);

    return 0;
}
